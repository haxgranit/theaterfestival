<p id="notice"><%= notice %></p>

<h1>Listing Artist Social Metadata</h1>

<table>
  <thead>
    <tr>
      <th>Twitter</th>
      <th>Facebook</th>
      <th>Linkedin</th>
      <th>Tumblr</th>
      <th>Snapchat</th>
      <th>Email</th>
      <th>Website</th>
      <th>Instagram</th>
      <th>Vine</th>
      <th>Artist</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @artist_social_metadata.each do |artist_social_metadatum| %>
      <tr>
        <td><%= artist_social_metadatum.twitter %></td>
        <td><%= artist_social_metadatum.facebook %></td>
        <td><%= artist_social_metadatum.linkedin %></td>
        <td><%= artist_social_metadatum.tumblr %></td>
        <td><%= artist_social_metadatum.snapchat %></td>
        <td><%= artist_social_metadatum.email %></td>
        <td><%= artist_social_metadatum.website %></td>
        <td><%= artist_social_metadatum.instagram %></td>
        <td><%= artist_social_metadatum.vine %></td>
        <td><%= artist_social_metadatum.artist %></td>
        <td><%= link_to 'Show', artist_social_metadatum %></td>
        <td><%= link_to 'Edit', edit_artist_social_metadatum_path(artist_social_metadatum) %></td>
        <td><%= link_to 'Destroy', artist_social_metadatum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Artist social metadata', new_artist_social_metadatum_path %>
