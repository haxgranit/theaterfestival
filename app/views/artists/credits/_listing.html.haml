%tr
  %td
    = attachment_image_tag(credit.creditable, :key_image, :fill, 50, 50)
  %td
    = link_to(credit.creditable.name, production_path(credit.creditable))
  %td
    = credit.position
  %td
    = credit.start_date.strftime('%D')
    - if credit.end_date.present?
      \-
      = credit.end_date.strftime('%D').row
