.container.container-fluid.header-image
  - if @company.banner_image_id.present?
    = attachment_image_tag(@company, :banner_image, :fill, 800, 200, class: 'img-fluid')
  - elsif @company.company_image_id.present?
    = attachment_image_tag(@company, :company_image, :fill, 800, 200, class: 'img-fluid')
