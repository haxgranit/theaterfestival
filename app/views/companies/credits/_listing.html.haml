%tr
  %td
    = attachment_image_tag(credit.artist, :profile_image, :fill, 100, 100)
  %td
    = link_to(credit.artist.stage_name, artist_path(credit.artist))
  %td
    = credit.position
  %td
    = credit.start_date.strftime('%D')
    - if credit.end_date.present?
      \-
      = credit.end_date.strftime('%D')
