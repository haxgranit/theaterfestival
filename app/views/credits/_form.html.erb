<%= simple_form_for(@model, wrapper: :horizontal_form) do |f| %>
    <%= f.input :stage_name, as: :fake %>
    <%= f.input :artist_id, as: :hidden %>
    <%= f.input :production_id, as: :string  %>
    <%= f.input :name %>
    <%= f.input :position %>
    <%= f.input :start_date %>
    <%= f.input :end_date %>
    <%= f.input :confirmed %>
    <%= f.button :submit %>
<% end %>
