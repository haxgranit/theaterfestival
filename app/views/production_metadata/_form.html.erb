<%= form_for(@production_metadata) do |f| %>
    <%= f.input :production_id %>
    <%= f.input :performance_type %>
    <%= f.input :info_heading %>
    <%= f.input :info %>
    <%= f.input :warnings %>
    <%= f.input :warning_info %>
    <%= f.input :running_time %>
    <%= f.input :intermissions %>
    <%= f.button :submit %>
<% end %>
