<%= simple_form_for(@showtime_ticket_metadata) do |f| %>
    <%= f.input :guaranteed_price %>
    <%= f.input :guaranteed_price_note %>
    <%= f.input :ticket_link %>
    <%= f.input :reservation_link %>
    <%= f.input :reservation_link_info %>
    <%= f.input :reservation_phone %>
    <%= f.input :reservation_phone_info %>
    <%= f.input :in_person_ticketing_info %>
    <%= f.input :special_event %>
    <%= f.input :special_event_info %>
    <%= f.input :additional_ticket_type %>
    <%= f.input :additional_ticket_type_info %>
    <%= f.input :notes %>
    <%= f.button :submit %>
<% end %>
