<%= bootstrap_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <%= f.email_field :email %>
    <%= f.password_field :password %>
    <%= f.check_box :remember_me %>
    <%= f.submit 'Sign in' %>
    <%= link_to 'Register', new_user_registration_path, class: 'btn btn-success' %>
<% end %>
